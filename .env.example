# YouTube Data API v3
YOUTUBE_API_KEY=your_youtube_api_key_here

# Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud Translation API Key (optional, for fallback)
# Set your service account JSON key directly here.
# BE CAREFUL with special characters and quotes if setting in a shell directly.
GOOGLE_CLOUD_CREDENTIALS='{ "type": "service_account", "project_id": "...", "private_key_id": "...", "private_key": "...", "client_email": "...", "client_id": "...", "auth_uri": "https://accounts.google.com/o/oauth2/auth", "token_uri": "https://oauth2.googleapis.com/token", "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs", "client_x509_cert_url": "https://www.googleapis.com/robot/v2/x509/" }'

# Webshare Proxy Credentials (optional but recommended to avoid IP bans)
WEBSHARE_PROXY_USERNAME=your_webshare_username_here
WEBSHARE_PROXY_PASSWORD=your_webshare_password_here
